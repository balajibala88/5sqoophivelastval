load data inpath '${INPUTPATH}' overwrite into table ${TBLNAME};

insert overwrite table ${EXTTBLNAME} partition(city)
select a.custid,a.firstname,a.lastname,a.age,a.createdt,a.transactamt,a.upddt,a.city from ${TBLNAME} a
union
select b.custid,b.firstname,b.lastname,b.age,b.createdt,b.transactamt,b.upddt,b.city from ${EXTTBLNAME} b where b.custid not in (select a.custid from ${TBLNAME} a);
